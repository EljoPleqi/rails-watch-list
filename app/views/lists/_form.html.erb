<div class="container form-control">

  <%= simple_form_for(@list) do |f|  %>
  <h3 class="pb-4">Create your list 👇🏻</h3>
    <div class="form-inputs">
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
      </div>
        <%= f.button :submit, class: 'btn btn-success' %>
  <% end %>
</div>
